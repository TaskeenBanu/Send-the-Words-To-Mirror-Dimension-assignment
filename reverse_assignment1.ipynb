   "source": [
    "def reverse(string):\n",
    "    string = [string[i] for i in range(len(string)-1, -1, -1)]\n",
    "    return \" \".join(string)\n",
    "s = \"edyoda\"\n",
    "print(\"the original string is : \", s)\n",
    "print(\"the reversed string(using reversed) is : \",reverse(s))\n"
   ]
